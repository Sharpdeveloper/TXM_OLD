<Window x:Class="TXM.GUI.Dialogs.OutputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Choose Information" Height="170" Width="150"
        WindowStyle="ToolWindow">
    <Grid>
        <CheckBox x:Name="CheckBoxLastResult" Content="Last Result" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" IsChecked="false"/>
        <CheckBox x:Name="CheckBoxCurrentTable" Content="Current Table" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" IsChecked="true"/>
        <CheckBox x:Name="CheckBoxCurrentPairings" Content="Current Pairings" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" IsChecked="false"/>
        <Button Content="Copy to Clipboard" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="114" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="10,95,0,0" VerticalAlignment="Top" Width="114" Click="Button_Click_1"/>
    </Grid>
</Window>
